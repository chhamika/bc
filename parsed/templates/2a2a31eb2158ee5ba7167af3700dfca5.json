{"components/blog/sidebar":"\r\n\r\n<div class=\"sidebarBlock\">\r\n  \r\n    <div class=\"sidebarBlock box-category module\">\r\n        <h5 class=\"block-title\"><i class=\"fa fa-diamond\"></i> {{lang 'category.label'}}</h5>\r\n        \r\n        <ul class=\"accordion\" id=\"accordion-category\">\r\n            {{#each categories}} \r\n            <li class=\"panel\">\r\n                {{#if  id '===' category.subcategories }}\r\n                    <a class=\"active\" href=\"{{url}}\" alt=\"{{name}}\" title=\"{{name}}\">{{name}}</a>\r\n                {{else}}\r\n                    <a  href=\"{{url}}\" alt=\"{{name}}\" title=\"{{name}}\">{{name}}</a>\r\n                {{/if}}\r\n\r\n                {{#if children}}\r\n                <span class=\"head\"><a  class=\"pull-right accordion-toggle collapsed\" data-toggle=\"collapse\" data-parent=\"#accordion-category\" href=\"#category{{id}}\"></a></span>\r\n                <ul  id=\"category{{id}}\" class=\"panel-collapse collapse \">\r\n                    {{#each children}}\r\n                    <li class=\"navList-subMenu-item\">\r\n                        <a class=\"navList-subMenu-action\" href=\"{{url}}\" alt=\"{{name}}\" title=\"{{name}}\">{{name}}</a>\r\n                         \r\n                    </li>\r\n                    {{/each}}\r\n                </ul>\r\n                {{/if}}\r\n\r\n            </li>\r\n            {{/each}}\r\n        </ul>\r\n    </div>\r\n\r\n    \r\n    <div class=\"module block-recent\">\r\n      <h5 class=\"block-title\"><i class=\"fa fa-file-text-o\"></i> {{lang 'blog.recent_posts'}}</h5>\r\n   \r\n      {{#if blog.posts}}\r\n        <div class=\"block-content\">\r\n         <ul class=\"navList\">\r\n             {{#each blog.posts}}\r\n              <li class=\"navList-item\">\r\n                 <a href=\"{{url}}\">{{title}}</a>\r\n                  <time >{{date_published}}</time>\r\n             </li>\r\n             {{/each}}\r\n         </ul>\r\n      </div>\r\n     {{/if}}\r\n   </div>\r\n     \r\n   <div class=\"module blog-tags\">\r\n        <h5 class=\"block-title\"><i class=\"fa fa-tag\"></i> {{lang 'sb_themes.blog.popular_tags'}}</h5>\r\n        <div class=\"block-content clearfix\">\r\n        {{#each blog.posts}}\r\n            {{#if tags}}\r\n                <ul class=\"tags\">\r\n                    {{#each tags}}\r\n                    <li class=\"tag\">\r\n                        <a href=\"{{url}}\">#{{name}}</a>\r\n                    </li>\r\n                    {{/each}}\r\n                </ul>\r\n            {{/if}}\r\n        {{/each}}\r\n        </div>\r\n   </div>\r\n   \r\n    <div class=\"module \">\r\n        <div class=\"block-content no-padding\">\r\n            <div class=\"banners\" >\r\n                <div>\r\n                    <a href=\"{{lang 'sb_themes.blog.banner_url'}}\" title=\"{{lang 'sb_themes.blog.banner_title'}}\">\r\n                      <img src=\"{{cdn (lang 'sb_themes.blog.image')}}\" alt=\"\">\r\n                    </a>\r\n                </div>\r\n                \r\n            </div>\r\n            \r\n        </div>\r\n    </div>\r\n\r\n  \r\n</div>\r\n\r\n"}
