{"components/sbthemes/common/subscription-form2":"<div class=\"newsletter-sidebar\">\r\n    <div class=\"form-head\">\r\n        <h5>{{lang 'newsletter.subscribe2'}}</h5>\r\n        <p>{{lang 'newsletter.subscribe_intro2'}}</p>\r\n    </div>\r\n    <form class=\"form\" action=\"{{urls.subscribe.action}}\" method=\"post\">\r\n        <fieldset class=\"form-fieldset\">\r\n            <input type=\"hidden\" name=\"action\" value=\"subscribe\">\r\n            <input type=\"hidden\" name=\"nl_first_name\" value=\"bc\">\r\n            <input type=\"hidden\" name=\"check\" value=\"1\">\r\n            <div class=\"form-field\">\r\n                <label class=\"form-label is-srOnly\" for=\"nl_email\">{{lang 'common.email_address'}}</label>\r\n                <div class=\"input-group justify-content-md-center\">\r\n\r\n                    <input class=\"form-input form-control\" id=\"nl_email\" name=\"nl_email\" type=\"email\" value=\"{{customer.email}}\" placeholder=\"{{lang 'newsletter.email_placeholder'}}\">\r\n                    <div class=\"input-group-btn\"> \r\n                        <button class=\"btn btn-primary button--primary\" type=\"submit\"><i class=\"fa fa-paper-plane\"></i></button>\r\n                    </div>\r\n                </div>\r\n            </div>\r\n        </fieldset>\r\n    </form>\r\n</div>\n"}
