tion\">\r\n                        {{#if theme_settings.show_product_wishlist}}\r\n                        <li class=\"item-wishlist navUser-item d-none d-sm-block\"><a class=\"cart-btn cart-button--wishlist\" href=\"{{urls.account.wishlists.all}}\">\r\n                            <svg class=\"icon-heart-o\" width=\"19\" height=\"19\" ><use xlink:href=\"#icon-heart-o\"></use></svg>\r\n                        </a> </li>\r\n                        {{/if}}\r\n                        \r\n                        {{#if theme_settings.show_product_compare}}\r\n                        <li class=\"item-compare navUser-item d-none d-sm-block\">\r\n                            <a class=\"cart-btn cart-button--compare navUser-item--compare\" href=\"{{urls.compare}}\" data-compare-nav> \r\n                                <svg class=\"icon-loop\" width=\"18\" height=\"18\"><use xlink:href=\"#icon-loop\"></use></svg>\r\n                            </a>\r\n                        </li>\r\n                        {{/if}}\r\n                        <li class=\"navUser-item item-cart\">\r\n                            <a\r\n                                class=\"cart-button cart-button--primary\"\r\n                                data-cart-preview\r\n                                data-dropdown=\"cart-preview-dropdown\"\r\n                                data-options=\"align:left\"\r\n                                href=\"{{urls.cart}}\">\r\n                                <svg class=\"icon-shopping-cart\" width=\"24\" height=\"24\"><use xlink:href=\"#icon-shopping-cart\"></use></svg> \r\n                                <span class=\"countPill{{#if cart.items}} countPill--positive{{/if}} cart-quantity\">{{cart.quantity}} {{lang 'header.txt_item'}}</span>\r\n                            </a>\r\n                            <div class=\"dropdown-menu\" id=\"cart-preview-dropdown\" data-dropdown-content aria-hidden=\"true\"></div>\r\n                        </li>\r\n                    <