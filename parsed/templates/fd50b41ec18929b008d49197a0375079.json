{"components/common/breadcrumbs":"<div class=\"sb-breadcrumbs breadcrumb-bg \">\n        <ul class=\"breadcrumb \">\n            {{#each breadcrumbs}}\n                <li class=\"breadcrumb-item {{#if @last}}is-active{{/if}}\">\n                    {{#if url}}\n                        {{#if @first}}\n                            <i class=\"fa fa-home\"></i>\n                         {{/if}}\n                        <a href=\"{{url}}\" class=\"breadcrumb-label\">{{name}}</a>\n                    {{else}}\n                        <span class=\"breadcrumb-label\">{{name}}</span>\n                    {{/if}}\n                </li>\n                \n            {{/each}}\n        </ul>\n        \n</div>\n"}
