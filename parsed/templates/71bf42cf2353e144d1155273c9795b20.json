{"components/sbthemes/module/sb_megamenu/horizontal/item-categories-lite":"<li class=\"navPages-item {{#if type-menu '==' 'mega'}}hasMegamenu{{/if}} {{#if position '==' 'right'}}float-right{{/if}}\">\n\t<a class=\"navPages-action\" href=\"{{url}}\">\n\t\t\n\t\t{{#if ../icon-awesome '!=' ' '}}\n\t\t\t<i class=\"fa fa-{{../icon-awesome }}\"></i>\n\t\t{{else if ../icon-image '!=' ' '}}\n\t\t\t<img data-sizes=\"auto\" class=\"navPages__icon lazyload\" src=\"{{cdn '/assets/img/loading.svg'}}\" data-src=\"{{cdn ../icon-image }}\" alt=\"icons\">\n\t\t{{/if}}\n\t\t{{#if ../badge '!=' ' '}}\n\t\t\t<span class=\"badge badge--{{../badge}}\">{{../badge}}</span>\n\t\t{{/if}} \n\t\t\n\t\t{{name}}\n\n\t</a>\n</li>\n"}
