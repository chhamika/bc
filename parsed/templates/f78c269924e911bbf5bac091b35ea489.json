{"components/sbthemes/common/quick-search":"\r\n    {{{ snippet \"forms_search\"}}}\r\n    <form class=\"sb-searchpro\" action=\"{{urls.search}}\">\r\n        <fieldset class=\"form-fieldset\">\r\n            <div class=\"input-group\">\r\n                <input class=\"form-control form-input\" data-search-quick name=\"search_query\" id=\"search_query\" data-error-message=\"{{lang 'search.error.empty_field'}}\" placeholder=\"{{lang 'search.quick_search.input_placeholder'}}\" autocomplete=\"off\">\r\n                <div class=\"input-group-append\"  >\r\n                    <button class=\"btn btn-outline-secondary\" id=\"btn-quickSearch\" type=\"submit\"><i class=\"fa fa-search\"></i></button>\r\n                </div>\r\n            </div>\r\n            \r\n        </fieldset>\r\n    </form>\r\n    <div class=\"dropdown dropdown--quickSearch\">\r\n        <section class=\"quickSearchResults \" data-bind=\"html: results\"></section>  \r\n    </div>\r\n   \r\n\r\n"}
