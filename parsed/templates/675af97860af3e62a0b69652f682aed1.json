{"components/sbthemes/module/sb_instagram/default":"<div class=\"module\">\r\n    {{#unless hide_heading}}\r\n    <div class=\"block-title\">\r\n        <h5 class=\"page-heading\">{{lang 'sb_themes.instagram.title'}}</h5>\r\n        <div class=\"heading-desc\">{{{lang 'sb_themes.instagram.description'}}}</div>\r\n    </div>\r\n    {{/unless}}\r\n    <div id=\"instafeed\" class=\"sb-instagram-gallery row\"></div>\r\n</div>\r\n\r\n<script type=\"text/javascript\" src=\"{{cdn '/assets/vendor/instafeed/instafeed.min.js'}}\"></script>\r\n<script type=\"text/javascript\">\r\n// <![[\r\n    (function() {\r\n        \"use strict\";\r\n        var feed = new Instafeed({\r\n            get: 'user',\r\n            userId: '{{lang \"sb_themes.instagram.user_id\"}}',\r\n            clientId: '{{lang \"sb_themes.instagram.client_id\"}}',\r\n            accessToken: '{{lang \"sb_themes.instagram.access_token\"}}',\r\n            resolution: 'standard_resolution',\r\n            template: '<div class=\"instagram-item col-4\"><a href=\"{'+'{link}}\" target=\"_blank\" id=\"{'+'{id}}\"><img src=\"{'+'{image}}\" /></a></div>',\r\n            sortBy: 'most-recent',\r\n            limit: 6,\r\n            links: false\r\n        });\r\n        feed.run();\r\n    })();\r\n// ]]>\r\n</script>\r\n"}
