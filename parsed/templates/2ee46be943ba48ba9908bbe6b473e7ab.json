{"components/sbthemes/custom/latest_blog_item":" <article class=\"blog-post \">\r\n\t\t\t\t    \r\n\r\n    <div class=\"blog-post-figure\">\r\n        {{#if post.thumbnail}}\r\n            <figure class=\"blog-thumbnail\">\r\n                <a href=\"{{post.url}}\">\r\n                    <img src=\"{{getImage post.thumbnail 'blog_size' }}\" alt=\"{{post.thumbnail.alt}}\" title=\"{{post.thumbnail.alt}}\">\r\n                </a>\r\n            </figure>\r\n        {{/if}}\r\n    </div>\r\n    <div class=\"blog-header\">\r\n    \t<p class=\"blog-date\"><i class=\"fa fa-clock-o\"></i> {{post.date_published}}</p>\r\n\r\n        <h2 class=\"blog-title\">\r\n            <a href=\"{{post.url}}\">{{post.title}}</a>\r\n        </h2>\r\n\r\n    </div> \r\n\r\n    <div class=\"blog-post-body\">\r\n        <p>\r\n        {{#if post.body}}\r\n            {{{post.body}}}\r\n        {{else}}\r\n            {{post.summary}}\r\n             &hellip; \r\n        {{/if}}\r\n        </p>\r\n        <div class=\"clear\"></div>\r\n        {{#if post.show_read_more}}\r\n                   <a href=\"{{url}}\" class=\"btn btn-secondary btn--readmore\">read more</a>\r\n        {{/if}}\r\n\r\n        {{#if show_tags}}\r\n            {{#if post.tags}}\r\n                <ul class=\"tags\">\r\n                    {{#each post.tags}}\r\n                    <li class=\"tag\">\r\n                        <a href=\"{{url}}\">#{{name}}</a>\r\n                    </li>\r\n                    {{/each}}\r\n                </ul>\r\n            {{/if}}\r\n        {{/if}}\r\n\r\n\r\n    </div>\r\n</article>\n"}
